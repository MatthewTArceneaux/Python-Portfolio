# Dictionary mapping English numbers to French
english_to_french = {
    "one": "un",
    "two": "deux",
    "three": "trois",
    "four": "quatre",
    "five": "cinq"
}

# Ask for user input
english_number = input("Enter an English number (one, two, three, four, five): ")

# Convert the input to lowercase
english_number = english_number.lower()

# Check if the input is in the dictionary
if english_number in english_to_french:
    # Output the corresponding French number
    print("French number:", english_to_french[english_number])
else:
    # Print an error message if the input is not in the dictionary
    print("Error: Input not recognized.")
